{% extends "base.html.twig" %}

{% block  carousell %}
    <header>
        <title>Bianco Blanco Page Photographie</title>
        <!-- carouselle principal-->
        <div id="boite-carouselle-menu" class="container-fluid">
{#            ca me permet de cree la ligne de changement sur mon carouselle#}
            <div
                    id="Carouselle-Menu"
                    class="carousel slide row"
                    data-ride="carousel">
                <div>
                    <!-- action du carouselle -->
                    <ol class="carousel-indicators">
                        <li
                                data-target="#Carouselle-Menu"
                                data-slide-to="0"
                                class="active">
                        </li>
                        <li data-target="#Carouselle-Menu" data-slide-to="1"></li>
                        <li data-target="#Carouselle-Menu" data-slide-to="2"></li>
                    </ol>

                    <!-- Main carouselle -->
                    <div class="carousel-inner ">
                        <div class="carousel-item main-carousel active ">
                            <img src="img/3000x3000/hh.png" class="d-block w-100 " id="photos" alt="sur se site vous pourez trouvez les derniers photos de blanco"/>
                            <div class="carousel-caption  d-md-block"></div>
                        </div>
                        <div class="carousel-item main-carousel">
                            <img src="img/photo/LG PHOTO/photo_8_lg.JPG"class="d-block w-100 img-fluid img-responsive" alt="vous pourez commander une cover d'album ou un clip street sur se site web"/>
                        </div>
                        <div class="carousel-item main-carousel">
                            <img
                                    src="IMG/photo/LG PHOTO/photo-10-lg.JPG"
                                    class="d-block w-100 img-fluid"
                                    alt="vous pourez trouvez les derniers Vétemens de la marque ELC sur se site web"/>
                        </div>
                    </div>
                    <!-- icon et action de slide carouselle -->
                    <a class="carousel-control-prev" href="#Carouselle-Menu" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    </a>
                    <a class="carousel-control-next m-0" href="#Carouselle-Menu" role="button"data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    </a >
                </div>
            </div>
        </div>
        <!-- navbar MENU -->
        <nav class="contenaire-fluid navbar">
            <div class="row justify-content-between align-items-center">
                <div class="col-sm-3 col-lg-3">
                    <img src="img/blanco.png" alt="" class="container-fluid " />
                </div>
                <div class="col-lg-6 text-light text-center font-weight-bold">
                    <!-- test card -->
                    <div class="cardd-zoom">
                        <ul class="row text">
                            <li class="sm-col-4 col-lg-4 p-"><a href={{ path("cover_list") }}>COVER</a></li>
                            <li class="sm-col-4 col-lg-4"><a href={{ path("photos_list") }}>PHOTO</a></li>
                            <li class="sm-col-4 col-lg-4"><a href={{ path("product_shop") }}>SHOP</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </nav>
    </header>
{% endblock  carousell %}

